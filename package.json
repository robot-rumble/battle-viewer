    "bootstrap": "^4.5.0",
    "bowser": "^2.11.0",
    "brotli": "https://github.com/foliojs/brotli.js/tarball/master",
    "buffer": "^6.0.3",
    "codemirror": "^5.46.0",
    "comlink": "^4.3.0",
    "core-js": "^3.10.1",
    "line-by-line": "^0.1.6",
