{
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es2019",
      "dom"
    ],
    "target": "es6",
    "importHelpers": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "paths": {
      "*": [
        "./types/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
